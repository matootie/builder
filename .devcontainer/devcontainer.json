{
  "name": "Builder",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "settings": {},
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "bradlc.vscode-tailwindcss",
    "skyapps.fish-vscode",
    "csstools.postcss",
    "ms-azuretools.vscode-docker",
    "ms-python.python"
  ],
  "forwardPorts": [3000, 3001, 1234, 6379],
  "remoteUser": "node",
  "features": {
    "git": "latest",
    "git-lfs": "latest",
    "github-cli": "latest",
    "aws-cli": "latest",
    "python": "3.10"
  }
}
